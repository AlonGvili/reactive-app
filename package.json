{
  "name": "reactive-app",
  "version": "0.0.1",
  "description": "Build large scale applications supported by visual tools",
  "repository": "git@github.com:christianalfoni/reactive-app",
  "author": "Christian Alfoni <christianalfoni@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:lib & npm run build:es",
    "build:lib": "tsc -p ./runtime --outDir lib --module commonjs",
    "build:es": "tsc -p ./runtime --outDir es --module es2015",
    "editor:watch": "tsc -w -p ./editor",
    "editor:dev": "parcel editor/index.html --port 5050"
  },
  "dependencies": {
    "electron": "^10.1.5",
    "mobx": "^6.0.3",
    "prettier": "^2.1.2"
  },
  "devDependencies": {
    "@types/prettier": "^2.1.5",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.2",
    "@types/pathfinding": "0.0.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.3",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.2.7",
    "lodash": "^4.17.20",
    "mobx-react": "^6.3.0",
    "parcel-bundler": "^1.12.4",
    "pathfinding": "^0.4.18",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.4.3",
    "react-icons": "^3.11.0",
    "react-resize-observer": "^1.1.1",
    "react-zoom-pan-pinch": "^1.6.1",
    "styled-components": "^5.2.0",
    "uuid": "^8.3.0",
    "ws": "^7.3.1",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "typescript": "^4.0.2"
  }
}
